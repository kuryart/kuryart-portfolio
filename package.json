{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "browserslist": [
    ">0.3%",
    "defaults",
    "supports es6-module",
    "maintained node versions"
  ],
  "scripts": {
    "db:push": "bunx drizzle-kit push:sqlite",
    "db:seed": "bun run src/model/store/seed.ts",
    "db:studio": "bunx drizzle-kit studio",
    "dev:tailwind": "concurrently \"bun run --hot src/main.ts\" \"bun run tailwind:dev\" \"bun run liveReload\"",
    "dev:uno": "concurrently \"bun run --hot src/main.ts\" \"bun run uno:dev\" \"bun run liveReload\"",
    "dev:gulp": "concurrently \"bun run --hot src/main.ts\" \"bun run gulp:dev\" \"bun run liveReload\"",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "lint": "eslint . --report-unused-disable-directives",
    "liveReload": "bunx beth-stack",
    "start:tailwind": "bun run tailwind && bun run src/main.ts",
    "start:uno": "bun run uno && bun run src/main.ts",
    "start:gulp": "bun run gulp && bun run src/main.ts",
    "tailwind:dev": "bunx tailwindcss -i ./src/css/base.css -o ./public/dist/style.css --watch",
    "tailwind": "bunx tailwindcss -i ./src/css/base.css -o ./public/dist/style.css",
    "gulp": "bunx gulp css",
    "gulp:dev": "bunx gulp css",
    "typecheck": "bunx --bun tsc",
    "uno:dev": "bunx --bun unocss --watch",
    "uno": "bunx --bun unocss"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^1.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint": "^8.44.2",
    "@types/gulp": "^4.0.17",
    "@types/node": "^20.9.2",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@unocss/transformer-variant-group": "^0.55.7",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^8.6.0",
    "browserlist": "^1.0.1",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "daisyui": "latest",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.14",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-isaacscript": "^3.5.5",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-imagemin": "^9.0.0",
    "gulp-import-css": "^0.1.3",
    "gulp-postcss": "^9.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^5.1.0",
    "gulp-uglify": "^3.0.2",
    "pino": "^8.15.1",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "precss": "^4.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unocss": "^0.57.5"
  },
  "dependencies": {
    "@bogeychan/elysia-logger": "0.0.9",
    "@elysiajs/cron": "^0.6.0",
    "@elysiajs/static": "^0.6.0",
    "@elysiajs/swagger": "^0.6.2",
    "@iconify-json/logos": "^1.1.37",
    "@iconify-json/lucide": "^1.1.128",
    "@libsql/client": "0.3.5-pre.13",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@lucia-auth/oauth": "^3.2.0",
    "@t3-oss/env-core": "^0.6.1",
    "@tlscipher/holt": "1.1.0",
