    "eslint-plugin-isaacscript": "^3.5.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint": "^8.49.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-import-css": "^0.1.3",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp": "^4.0.2",
