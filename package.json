{
  "name": "test",
  "module": "src/main.ts",
  "type": "module",
  "browserslist": [
    ">0.3%",
    "defaults",
    "supports es6-module",
    "maintained node versions"
  ],
  "homepage": "https://kuryart.github.io/kuryart.github.io",
  "scripts": {
    "build": "bun build ./src/main.ts --outdir ./public/dist --minify --splitting --target bun",
    "dev": "concurrently \"bun run --hot src/main.ts\" \"bun run gulp:dev\" \"bun run liveReload\"",
    "format:check": "prettier --check .",
    "format": "prettier --write . --list-different",
    "gulp:dev": "bunx gulp dev",
    "gulp": "bunx gulp build",
    "lint:fix": "eslint . --report-unused-disable-directives --fix",
    "lint": "eslint . --report-unused-disable-directives",
    "liveReload": "bunx beth-stack",
    "start": "bun run gulp && bun run src/main.ts",
    "typecheck": "bunx --bun tsc"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@kitajs/ts-html-plugin": "^1.0.1",
