    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.16",
    "browserlist": "^1.0.1",
    "bun-types": "latest",
    "concurrently": "^8.2.1",
    "daisyui": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-turbo": "^1.10.14",
